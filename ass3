#include <stdio.h>

int main() {
    int i,n,a[10],max,min,dif;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    max=a[0];min=a[0];
    for(i=1;i<n;i++)
    {
    {
        if(a[i]>max)
        max=a[i];
    }
    {
        if(a[i]<min)
        min=a[i];
    }
    }
     
     dif=max-min;
     printf("difference=%d",dif);
    return 0;
}
